{
  "name": "xdrip-js-logger",
  "version": "0.3.1",
  "description": "Logger xdrip openaps",
  "main": "logger/index.js",
  "author": "Paul Dickens",
  "license": "MIT",
  "scripts": {
    "test": "make test",
    "go": "go build calc-noise.go",
    "link": "ln -s /root/src/xdrip-js-logger/bin/calibrate.sh /usr/local/bin/calibrate",
    "global-install": "npm install && sudo npm install -g && sudo npm link && sudo npm link xdrip-js-logger && npm run go && npm run link "
  },
  "dependencies": {
    "xdrip-js": "thebookins/xdrip-js#v1.1.2"
  },
  "bin": {
    "xdrip-get-entries": "./xdrip-get-entries.sh",
    "post-xdripAPS": "./post-xdripAPS.sh",
    "post-ns": "./post-ns.sh",
    "calc-calibration": "./calc-calibration.sh",
    "calibrate": "./bin/calibrate.sh",
    "calc-calibration": "./calc-noise.sh"
  },
  "homepage": "https://github.com/efidoman/xdrip-js-logger",
  "config": {
    "blanket": {
      "pattern": [
        "bin"
      ],
      "data-cover-never": [
        "node_modules"
      ]
    }
  }
}


